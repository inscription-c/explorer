        "esbuild": "^0.17.5",
        "eslint": "^8.5.0",
        "eslint-plugin-vue": "^8.2.0",
        "prettier": "^2.5.1",
        "sass": "^1.49.9",
        "sass-loader": "^10.0.0",
        "typescript": "~4.5.5",
        "vite": "4.1.1",
        "vue-cli-plugin-vuetify": "2.5.5",
        "vue-tsc": "1.0.3",
        "vuetify-loader": "^2.0.0-alpha.9"
