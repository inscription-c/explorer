        "deploy:testnet": "npx wrangler pages deploy ./dist --project-name c-ins-test --branch testnet",
        "typecheck": "vue-tsc --noEmit",
        "lint": "eslint . --ext .vue,.ts --fix --ignore-path .gitignore"
    },
    "dependencies": {
        "@sentry/vue": "^7.102.1",
        "axios": "^1.6.7",
        "date-fns": "^2.29.3",
        "dayjs": "^1.11.10",
        "maska": "^1.5.0",
        "pinia": "2.0.11",
        "vee-validate": "4.6.7",
        "vite-plugin-vuetify": "1.0.2",
        "vue": "3.2.31",
        "vue-highlight-code": "0.1.13",
        "vue-i18n": "^9.2.2",
        "vue-router": "4.0.12",
        "vue-scrollto": "^2.20.0",
        "vue-tabler-icons": "2.9.0",
        "vuetify": "3.3.3",
        "vuetify-sonner": "^0.3.13",
        "yup": "^0.32.11"
